<?xml version="1.0" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" default-lazy="false">
  <class
name="WindowsForm_ORM.Employee, WindowsForm_ORM"
discriminator-value="0" table="employee"
>
<id
	name="id" type="int"
	unsaved-value="null"
	>
